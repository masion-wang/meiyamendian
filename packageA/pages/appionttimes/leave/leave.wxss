page {
  background-color: #f3f3f3;
  height: 100%;
}

/* 轮播图日历*/
.swiper-box .wx-swiper-dots.wx-swiper-dots-horizontal {
  margin-bottom: -20rpx;
  z-index: 999;
}

.swiper-box .wx-swiper-dot {
  width: 40rpx;
  display: inline-flex;
  height: 10rpx;
  margin-left: 20rpx;
  justify-content: space-between;
}

.swiper-box .wx-swiper-dot::before {
  content: '';
  flex-grow: 1;
  background: rgba(255, 255, 255, 0.8);
  border-radius: 8rpx
}

.swiper-box .wx-swiper-dot-active::before {
  background: #f8a43b;
}

.swiper-box {
  /* border: 1px solid black; */
  border-radius: 10rpx;
  height: 50vh;
}

.weui-dialog__bd {
  padding: 0;
  /* margin-left:30rpx;
  margin-right:30rpx; */
}

.swiperI {
  width: 100%;
  /* border: 1px solid black; */
  /* display: flex;
  justify-content: center;
  align-items: center; */
}

.recentThreeMonth {
  color: #eaa33e;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
  border-top: 1px solid rgb(207, 202, 202);
  border-bottom: 1px solid rgb(207, 202, 202);
}


/*上面一部分*/
.up {
  width: 94%;
  /* height: 500rpx; */
  /* border: 1px solid black; */
  margin: 0 auto;
  margin-top: 30rpx;
  background-color: white;
  border-radius: 5px;
}

.selectDate {
  display: flex;
  flex-direction: column;
  /* border: solid 1px red; */

}

.selectDateTitle {
  width: 100%;
  font-size: 28rpx;
  line-height: 60rpx;
  text-indent: 20rpx;
  /* border: solid 1px red; */
}

.srcollView {
  padding-bottom: 2vh;
}

scroll-view ::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
  display: none;
}

.box {
  width: 100%;
  overflow: hidden;
  /* padding-top: 20rpx; */
  padding-bottom: 20rpx;
  background: #fff;
  white-space: nowrap;
  /* border:1px solid black; */
}

.item {
  width: 160rpx;
  height: 75rpx;
  text-align: center;
  margin-left: 15rpx;
  display: inline-block;
}

.movedown {
  position: relative;
  top: 15rpx;
}

.item:nth-last-child(1) {
  margin-right: 15rpx;
}

.sonofItem {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  border: 1px solid #f3f3f3;
  border-radius: 30px;
  background-color: #f3f3f3;
  color: #575454;
  height: 75rpx;
}

.activesonofItem {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  border-radius: 30px;
  border: 1px solid #e48144;
  background-color: #fff6f4;
  color: #e48144;
  height: 100%;
}

.week {
  width: 100%;
  font-size: 24rpx;
  line-height: 30rpx;
  margin-top: 10rpx;
  /* border: solid 1px red; */
}

.date {
  width: 100%;
  font-size: 20rpx;
  line-height: 30rpx;
  margin: 5rpx 0;
  /* border: solid 1px red; */
}

.parent {
  width: 100%;
  margin: 0 auto;
  /* border: 1px solid black; */
  display: flex;
  align-items: center;
  flex-direction: column;
  flex-wrap: wrap;
}

.son {
  width: 90%;
  height: 70rpx;
  font-size: 25rpx;
  border-radius: 5px;
  background-color: #f3f3f3;
  color: #575454;
  text-align: center;
  line-height: 70rpx;
  margin-top: 20rpx;
  border: 1px solid #fffefe;
}

.activeson {
  border: 1px solid #e48144;
  background-color: #fff6f4;
  color: #e48144;
}

.tip {
  margin: 0 auto;
  width: 90%;
  /* border: 1px solid black; */
  border-radius: 5px;
  margin-top: 40rpx;
  margin-bottom: 20rpx;
  display: flex;
}

.icon {
  width: 25rpx;
  height: 25rpx;
  position: relative;
  top: 6rpx;
  margin-right: 8rpx;
}

.cost {
  display: flex;
  align-items: center;
  font-size: 25rpx;
  color: #8d8d8d;
  width: 80%;
}

.one,
.two {
  width: 60%;
}

.down {
  width: 94%;
  /* height: 500rpx; */
  /* border: 1px solid black; */
  border-radius: 5px;
  margin: 0 auto;
  margin-top: 30rpx;
  padding-bottom: 20rpx;
  background-color: white;
}

.textInput {
  width: 94%;
  height: 22vh;
  background-color: #f3f3f3;
  margin: 0 auto;
  margin-top: 10rpx;
  padding: 10rpx;
  border-radius: 5px;
}

.buttons {
  width: 94%;
  /* background-color: white; */
  margin: 0 auto;
  display: flex;
  /* border: 1px solid black; */
}

.dayOff {
  width: 300rpx;
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 30px;
  /* border:1px solid black; */
  margin: 0 auto;
  margin-top: 45rpx;
  margin-bottom: 25rpx;
  border: 1px solid #e48144;
  color: #e48144;
}

.button {
  width: 300rpx;
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 30px;
  /* border:1px solid black; */
  margin: 0 auto;
  margin-top: 45rpx;
  margin-bottom: 25rpx;
  color: white;
  background: -webkit-linear-gradient(right, rgb(250, 106, 4), rgb(237, 76, 4));
  background: -o-linear-gradient(right, rgb(250, 106, 4), rgb(237, 76, 4));
  background: -moz-linear-gradient(right, rgb(250, 106, 4), rgb(237, 76, 4));
  background: -mos-linear-gradient(right, rgb(250, 106, 4), rgb(237, 76, 4));
}

.flex {
  display: flex;
  align-items: center;
}

.opacity {
  opacity: 0;
}

.cover {
  position: fixed;
  left: 0;
  top: 0;
  z-index: 10;
  width: 100%;
  height: 100%;
  opacity: 0.5;
  background-color: transparent;
}